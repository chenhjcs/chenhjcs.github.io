        <jsScript_script>var timeSpanArr = TimeSpan.split(';');
//var timeSpanArr = TimeSpan.split(';');

var result = new Array(24 * 60);
for (var i = 0; i &lt; 24 * 60; i++) {
    result[i] = 0;
}


for(var i = 0; i &lt; timeSpanArr.length; i++){
    [strStartTime, strEndTime] = timeSpanArr[i].split(',');

    var fixDate = "2018-01-01 ";
    var fixTime = "00:00:00.000";

    var fixTime = str2date(fixDate + fixTime, "yyyy-MM-dd HH:mm:ss.SS");
    var startTime = str2date(fixDate + strStartTime, "yyyy-MM-dd HH:mm:ss.SS");
    var endTime = str2date(fixDate + strEndTime, "yyyy-MM-dd HH:mm:ss.SS");

    var starIndex = floor((startTime.getTime() - fixTime.getTime()) / 60000.0);
    var endIndex = ceil((endTime.getTime() - fixTime.getTime() - 1) / 60000.0);

    for (var j = starIndex; j &lt; endIndex; j++) {
        result[j] = 1;
		
    }
}

var TimeDistribution = result.join(',');</jsScript_script>
