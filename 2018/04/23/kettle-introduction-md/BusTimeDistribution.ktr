    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var strBeginDate = date2str(BeginDate, "yyyy-MM-dd");
var strEndDate = date2str(EndDate, "yyyy-MM-dd");

var BeginTime = date2str(BeginDate, "HH:mm:ss.SS");
var EndTime = date2str(EndDate, "HH:mm:ss.SS");


if(strBeginDate === strEndDate) {

	var ChargeDate = strBeginDate;
	var TimeSpan = BeginTime  + ','+ EndTime;

}else{

	var ChargeDate2 = strBeginDate;
	var TimeSpan2 = BeginTime  + ','+ "23:59:59.999";
    var newRow2 = createRowCopy(getOutputRowMeta().size());
    var rowIndex = getInputRowMeta().size();
    newRow2[rowIndex++] = ChargeDate2;
    newRow2[rowIndex++] = TimeSpan2;
    putRow(newRow2);

	var ChargeDate = strEndDate;
	var TimeSpan = "00:00:00.000"  + ','+  EndTime;

}</jsScript_script>
      </jsScript>
